<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="Information" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
            <font>
                <Font name="System Bold" size="28.0" />
            </font>
        </Label>
        <Label text="Add your own notes and information to be displayed in tabs here." AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="50.0">
            <font>
                <Font name="System Italic" size="24.0" />
            </font>
        </Label>
        <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
            <tabs>
                <Tab text="Add/Edit Tabs">
                    <graphic>
                        <ImageView pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="/images/new.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <content>
                        <AnchorPane/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>